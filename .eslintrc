rules:
  strict:
    - 1
  indent:
    - 2
    - 2
  quotes:
    - 2
    - double
  linebreak-style:
    - 2
    - unix
  semi:
    - 2
    - never
  no-implicit-coercion:
    - 2
  no-unused-vars:
    - 2
  no-sync:
    - 2
  func-names:
    - 1
  one-var:
    - 0
  max-len:
    - 1
    - 121
  max-depth:
    - 1
    - 2
env:
  es6: true
  node: true
parser: "babel-eslint"
extends:
  - "eslint:recommended"
  - "plugin:flowtype/recommended"
plugins:
  - "flowtype"
